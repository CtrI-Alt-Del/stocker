  "dependencies": {
    "@fastify/cookie": "11.0.1",
    "@fastify/cors": "10.0.1",
    "@fastify/jwt": "9.0.1",
    "@fastify/multipart": "9.0.1",
    "@fastify/websocket": "11.0.1",
    "@google/generative-ai": "0.21.0",
    "@prisma/client": "^5.19.1",
    "@stocker/core": "*",
    "@stocker/email": "*",
    "@stocker/validation": "*",
    "@supabase/supabase-js": "^2.45.4",
    "bcrypt": "5.1.1",
    "bull": "4.16.4",
    "exceljs": "4.4.0",
    "fastify": "5.1.0",
    "node-cron": "3.0.3",
    "react": "18.3.1",
    "resend": "4.0.1-alpha.0",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "@stocker/typescript-config": "*",
